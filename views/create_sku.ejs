<%- include('header.ejs') %>

<center>
	<div class="jumbotron">
		<h1>Create Order</h1>
	</div>
</center>
<div class="span9 offset1">
	<form id="form">
		<div class="form-group row">
			<label class="col-sm-2 col-form-label" for="type">Type of order</label>
			<div class="col-sm-10">
				<select id="type" class="form-control">
					<option>Quote</option>
					<option>Purchase Order</option>
				</select>
			</div>
		</div>
		<div class="form-group">
			<label class="col-sm-2 col-form-label" id="id_label" for="id">Order ID</label>
			<input type="text" class="form-control" id="id" placeholder="ORD0001">
		</div>
	</form>
</div>

<%- include('footer.ejs') %>